- _.each Coven.Constants.SOURCES, (source) =>
  li
    - dashified = Coven.Helpers.dashify(source)
    - if !@cookie? || @cookie.indexOf(dashified) >= 0
      input type='checkbox' id="#{dashified}" checked='checked' value="#{dashified}"
    - else
      input type='checkbox' id="#{dashified}" value="#{dashified}"
    label for="#{dashified}" class="#{dashified}" = source
